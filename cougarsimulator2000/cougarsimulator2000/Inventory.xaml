<Window x:Class="cougarsimulator2000.Inventory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cougarsimulator2000"
        mc:Ignorable="d"
        Title="Inventory" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <Image Source="{StaticResource M1860}" Margin="50,20"/>
            <Button x:Name="btnReturn" Content="Return" Click="btnReturn_Click" />
        </StackPanel>
    </Grid>
</Window>
