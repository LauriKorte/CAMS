        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cougarsimulator2000"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Closing="Window_Closing">
    <Window.Resources>
        <BitmapImage x:Key="bg_grass1" UriSource="pack://application:,,,/Resources/grass1.png" />
        <BitmapImage x:Key="ac_cougar" UriSource="pack://application:,,,/Resources/cougar.png" />
        <BitmapImage x:Key="bg_box" UriSource="pack://application:,,,/Resources/box.png" />
    </Window.Resources>

    <Grid x:Name="TileMap">
    </Grid>
</Window>
